{
  "name": "silent-abyss",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:pages": "vite build --base=/silent-abyss/",
    "preview": "vite preview",
    "deploy": "npm run build:pages && gh-pages -d dist",
    "deploy:ci": "npm run build:pages && gh-pages -d dist -r https://x-access-token:${GITHUB_TOKEN}@github.com/${GITHUB_REPOSITORY}.git",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "three": "^0.180.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "autoprefixer": "^10.4.24",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  }
}
